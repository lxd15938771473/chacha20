chakey : [256] -> [256]
chakey key = out'
    where key32 = groupBy`{32} key
          out = [ reverse (split`{4} keyt) | keyt <- key32 ]
          out' = join (join out)

chacounter : [96] -> [96]
chacounter key = out'
    where key32 = groupBy`{32} key
          out = [ reverse (split`{4} keyt) | keyt <- key32 ]
          out' = join (join out)



